<div class="banner">
<p>Wikinimous</p>
<%=link_to "Create Article", new_article_path%>
</div>
<div class="d-flex justify-content-end">
<%= form_for(@article) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit %>
<% end %>
<%=link_to "Back", root_path%>
</div>
